# ///// PROPERTY SHAPES FOR WorkTitlePropertyGroup START HERE. /////////////////////////////////////////////////////////////

raremat_monograph_form:WorkForm_arm_hasPreferredTitle 
    a sh:PropertyShape ;
    sh:path arm:hasPreferredTitle ;
    sh:class bf:Title ;
    tosh:editWidget swa:NestedObjectEditor ;
    sh:description "Record PreferredTitles here; there can be only one preferred title per language." ;
    sh:group raremat_monograph_property_groups:WorkTitlePropertyGroup ;
    sh:name "Preferred title" ;
    sh:nodeKind sh:IRI ;
    sh:order "0"^^xsd:int .

raremat_monograph_form:WorkForm_bf_title 
    a sh:PropertyShape ;
    sh:path bf:title ;
    sh:class bf:Title ;
    tosh:editWidget swa:NestedObjectEditor ;
    sh:description "Record non-PreferredTitles here. Use Preferred Title to capture a preferred title." ;
    sh:group raremat_monograph_property_groups:WorkTitlePropertyGroup ;
    sh:name "Other title" ;
    sh:nodeKind sh:IRI ;
    sh:order "1"^^xsd:int .


# ///// PROPERTY SHAPES FOR WorkActivityPropertyGroup START HERE. /////////////////////////////////////////////////////////////

raremat_monograph_form:WorkForm_activity_hasActivity 
    a sh:PropertyShape ;
    sh:path activity:hasActivity ;
    sh:name "Agent activity or role" ;
    sh:order "0"^^xsd:int ;
    sh:group raremat_monograph_property_groups:WorkActivityPropertyGroup ;
    tosh:editWidget swa:NestedObjectEditor ;
    sh:or ( 
        [
            sh:class activity:AuthorActivity ;
        ]
        [
            sh:class activity:CreatorActivity ;
        ]
        [
            sh:class activity:AttributionActivity ;
        ]
        [
            sh:class activity:CompilerActivity ;
        ]
        [
            sh:class activity:ContributorActivity ;
        ]
        [
            sh:class activity:CopyrightHolderActivity ;
        ]
        [
            sh:class activity:EditorActivity ;
        ]
        [
            sh:class activity:IllustratorActivity ;
        ]
        [
            sh:class activity:ArtistActivity ;
        ]
        [
            sh:class activity:PatronActivity ;
        ]
        [
            sh:class activity:SponsorActivity 
        ]
        [
            sh:class activity:TranslatorActivity ;
        ]
        [
            sh:class activity:Activity ;
        ]
        ) .
  
  
# ///// PROPERTY SHAPES FOR WorkSubjectGenreFormPropertyGroup START HERE. /////////////////////////////////////////////////////



raremat_monograph_form:WorkForm_bf_subject 
    a sh:PropertyShape ;
    sh:path bf:subject ;
    sh:group raremat_monograph_property_groups:WorkSubjectGenreFormPropertyGroup ;
    sh:name "Subject" ;
    sh:nodeKind sh:IRI ;
    sh:order "0"^^xsd:int ;
    raremat_monograph_form:lookup raremat_monograph_form:LD4LookupService ;
    sh:or (
        [
                sh:class bf:Work ;
        ]
        [
                sh:class bf:Instance ;
        ]
        [
                sh:class bf:Item ;
        ]
        [
                sh:class bf:Temporal ;
        ]
        [
                sh:class bf:Agent ;
        ]
        [
                sh:class lingvo:Lingvo ;
        ]
        [
                sh:class skos:Concept ;
        ]
        [
                sh:class gn:Feature ;
        ]
        [
                sh:class bf:Event ;
        ]
      ) .

raremat_monograph_form:WorkForm_bf_genreForm 
    a sh:PropertyShape ;
    sh:path bf:genreForm ;
    sh:class skos:Concept ;
    sh:group raremat_monograph_property_groups:WorkSubjectGenreFormPropertyGroup ;
    sh:name "Form Genre" ;
    sh:nodeKind sh:IRI ;
    sh:order "1"^^xsd:int ;
    raremat_monograph_form:lookup raremat_monograph_form:LD4LookupService .

# The following sh:Property with ( sh:path bf:genreForm madsrdf:isMemberOfMADSScheme ) isn't meant to be a field in the form, but rather state a restriction on the use of bf:genreForm to instances from http://id.loc.gov/authorities/genreForms. 

raremat_monograph_form:WorkForm_bf_genreForm_madsrdf_isMemberOfMADSScheme 
    a sh:PropertyShape ;
    sh:path ( bf:genreForm madsrdf:isMemberOfMADSScheme ) ;
    sh:hasValue <http://id.loc.gov/authorities/genreForms> ;
    sh:description "Values for form genre terms" ;
    sh:group raremat_monograph_property_groups:WorkSubjectGenreFormPropertyGroup ;
    sh:name "LCGFT Form Genre Terms" ;
    sh:nodeKind sh:IRI ;
    skos:editorialNote "Use look up service; LCGFT is too big to load directly into an RDF editor, and more importantly we don't want to create MADSAuthorities, only link to them." .
 
